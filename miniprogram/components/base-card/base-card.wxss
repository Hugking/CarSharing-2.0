.container {
    margin: 0 10rpx;
    background: #fff;
    border-radius: 10rpx;
}

.address-box-1 {
    display: flex;
    justify-content: center;
    align-content: center;
    padding: 10rpx;
    width: 100%;
}

.address-item {
    padding: 10rpx;
    font-size: 30rpx;
    width: 45%;
    border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
}

.start {
    display: flex;
    justify-content: center;
}

.end {
    display: flex;
    justify-content: center;
}

.icon {
    width: 10%;
    display: flex;
    justify-content: center;
    align-items: center;
}

.go-icon {
    height: 40px;
    width: 40px;
    padding: 10rpx;
    background-size: 40px 40px;
    background-image:url('data:image/png;base64,');
}

.time-box-1 {
    padding: 10rpx;
    display: flex;
    justify-content: center;
    align-content: center;
    border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
}

.time {
    padding: 10rpx;
}


/* type2 */

.address-item-2 {
    display: flex;
    align-items: center;
    padding: 20rpx;
}

.start-icon {
    display: inline-block;
    width: 20rpx;
    height: 20rpx;
    background-color: #0081ff;
    vertical-align: middle;
    margin-right: 15rpx;
    border-radius: 20rpx;
}

.end-icon {
    display: inline-block;
    width: 20rpx;
    height: 20rpx;
    background-color: #1cbbb4;
    vertical-align: middle;
    margin-right: 15rpx;
    border-radius: 20rpx;
}

.address-detail {
    width: 100%;
    border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
    padding: 20rpx;
    font-size: 32rpx;
}

.time-box-2 {
    display: flex;
    align-items: center;
    padding: 10rpx;
    margin-left: 10rpx;
}

.time-icon {
    display: inline-block;
    width: 20rpx;
    height: 20rpx;
    background-color: #6739b6;
    vertical-align: middle;
    margin-right: 15rpx;
    border-radius: 20rpx;
}

.time-title {
    width: 100%;
    border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
    padding: 20rpx;
    font-size: 32rpx;
}